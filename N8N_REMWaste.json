      "parameters": {
        "jsCode": "try {\n  // Acc√®s aux donn√©es dans body\n  const lead = items[0].json.body; \n  \n  const required = [\"full_name\", \"email\", \"budget\", \"interest_level\"];\n  const missing = required.filter(f => !lead[f]);\n\n  if (missing.length > 0) {  \n    throw new Error(\"Champs manquants : \" + missing.join(\", \"));\n  }\n\n  // Normalisation\n  const interest = lead.interest_level?.toString().toLowerCase().trim() || '';\n  const budget = parseFloat(lead.budget) || 0;\n\n  // Calcul du score\n  let score = \"Cold\";\n  if (budget >= 5000 && interest === \"high\") {\n    score = \"Hot\";\n  } else if (budget >= 3000) {\n    score = \"Warm\";\n  }\n\n  return [{\n    json: {\n      ...lead,\n      budget,\n      interest_level: interest,\n      score,\n      timestamp: new Date().toISOString(),\n      missing_fields: missing\n    },\n    pairedItem: { item: 0 }\n  }];\n} catch (error) {\n  return [{\n    json: { \n      error: error.message, \n      raw: items[0].json,\n      stack: error.stack \n    },\n    pairedItem: { item: 0 }\n  }];\n}"
      },
